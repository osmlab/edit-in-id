{
    "manifest_version": 2,
    "name": "Edit with iD",
    "version": "1.0.0",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*.tiles.mapbox.com/v3/*/page.html",
                "https://tiles.mapbox.com/*/edit/*"
            ],
            "js": ["d3.min.js", "mapbox.js"]
        }
    ]
}